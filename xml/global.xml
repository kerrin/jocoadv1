  <scripts>
    <script id="_onNewGame">
      <cache field="first_leave" value="start"/>
      <cache field="guard_in_ot" value="false"/>
      <cache field="guard_jacket" value="guard"/>
      <cache field="guard_key" value="in_jacket"/>
      <cache field="guard_ipod" value="in_jacket"/>
      <cache field="havePills" value="false"/>
      <cache field="laptop_song" value="start"/>
      <cache field="tod_song" value="start"/>
      <cache field="bought_sonic" value="start"/>
      <cache field="pirate_jokes_left" value="5"/>
      <cache field="stethoscope" value="1"/>
      <cache field="robots" value="ignoring"/>
      <cache field="endGame" value="no"/>
      <cache field="lookedAtWepMan" value="0"/>
      <cache field="gotSquib" value="0"/>
      <cache field="Flooded" value="-1"/>
      <game room="Tutorial"/>
    </script>
    <script id="_onStartGame">
      <game actor="ted"/>
      <game voices="1"/>
      <keyboard addKeyScript="i">
        <debug echo="Inventory..ho!"/>
        <inventory display="toggle"/>
      </keyboard>
      <keyboard addKeyScript="m">
        <menu display="toggle"/>
      </keyboard>
      <keyboard addKeyScript="h">
        <menu display="toggle"/>
      </keyboard>
      <keyboard addKeyScript="d">
        <debug enabled="0" display="toggle"/>
      </keyboard>
      <ui load="ui_layout"/>
      <method define="prepare_dialogue" before="1">
        <logic>
          <if eval="@before EQ 1">
            <debug echo="Dialogue..disable inventory"/>
            <keyboard disableKey="i"/>
            <inventory display="hide"/>
            <layer target="ui_layout:inventory" state="hide"/>
          </if>
          <else>
            <debug echo="Dialogue..enable inventory"/>
            <keyboard enableKey="i"/>
            <layer target="ui_layout:inventory" state="show"/>
          </else>
        </logic>
      </method>
    </script>
    <script id="afterDialogue">
      <debug echo="Dialogue..enable inventory"/>
      <keyboard enableKey="i"/>
      <keyboard enableKey="m"/>
      <layer target="ui_layout:inventory" state="show"/>
      <layer target="ui_layout:menu" state="show"/>
      <layer target="ui_layout:HelpMe" state="show"/>
    </script>
    <script id="beforeDialogue">
      <debug echo="Dialogue..disable inventory"/>
      <keyboard disableKey="i"/>
      <keyboard disableKey="m"/>
      <inventory display="hide"/>
      <layer target="ui_layout:inventory" state="hide"/>
      <layer target="ui_layout:menu" state="hide"/>
      <layer target="ui_layout:HelpMe" state="hide"/>
    </script>
  </scripts>
